En esta presentación veremos diferentes fórmulas matemáticas.

\end{frame}

\section{Segunda Sección}

\begin{frame}

\frametitle{Segunda Sección}

\begin{block}{Ejemplo}
  \begin{itemize}
  \item
  $x^2+y^2=0$
  \pause

  \item
  $S_n=a_1+\cdots + a_n = \sum_{i=1}^n a_1$
  \pause

  \item
   $x^{2+x}-5x^2+5=0$
  \pause

  \item
  $\sqrt{b^2-4ac}=2$
  \pause

  \item
   $\frac{x^3+6-x}{2-x^2}$
  \pause

  \end{itemize}
\end{block}

\end{frame}

